{
  "name": "todoist-lorem-ipsum",
  "version": "1.0.0",
  "description": "UI Extension for Todoist that generates Lorem Ipsum content",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "start": "node dist/main",
    "dev": "nodemon src/app.ts",
    "dev:simple": "nodemon src/app_simple.ts",
    "test": "jest --passWithNoTests",
    "format-check": "npx prettier --check .",
    "format-fix": "npx prettier --write .",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "ts-compile-check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doist/todoist-integrations-examples.git"
  },
  "author": "Doist Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Doist/todoist-integrations-examples/issues"
  },
  "homepage": "https://github.com/Doist/todoist-integrations-examples#readme",
  "dependencies": {
    "@doist/prettier-config": "^3.0.5",
    "@doist/todoist-api-typescript": "2.1.2",
    "@doist/ui-extensions-core": "4.1.1",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lorem-ipsum": "^2.0.8"
  },
  "devDependencies": {
    "@doist/tsconfig": "1.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.13",
    "@types/node": "18.11.9",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "source-map-support": "0.5.21",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16.11.1",
    "npm": ">=8.0.0"
  }
}

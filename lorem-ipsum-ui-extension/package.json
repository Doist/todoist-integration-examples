{
    "name": "todoist-lorem-ipsum",
    "version": "1.0.0",
    "description": "UI Extension for Todoist that generates Lorem Ipsum content",
    "scripts": {
        "clean": "rimraf dist",
        "build": "run-s clean typescript:build",
        "start": "node dist/main",
        "dev": "nodemon src/app.ts",
        "check": "run-p *:check",
        "format:check": "npx prettier --check .",
        "format:fix": "npx prettier --write .",
        "typescript:check": "tsc --noEmit",
        "typescript:build": "tsc"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Doist/todoist-integrations-examples.git"
    },
    "author": "Doist Developers",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Doist/todoist-integrations-examples/issues"
    },
    "homepage": "https://github.com/Doist/todoist-integrations-examples#readme",
    "dependencies": {
        "@doist/integrations-common": "2.0.0",
        "@doist/todoist-api-typescript": "2.2.0",
        "@doist/ui-extensions-core": "4.2.1",
        "body-parser": "1.20.3",
        "crypto-js": "4.2.0",
        "dotenv": "16.4.7",
        "express": "4.21.2",
        "lorem-ipsum": "2.0.8"
    },
    "devDependencies": {
        "@doist/prettier-config": "4.0.0",
        "@doist/tsconfig": "2.0.0",
        "@types/crypto-js": "4.2.2",
        "@types/express": "5.0.0",
        "@types/node": "22.13.1",
        "nodemon": "3.1.9",
        "npm-run-all2": "7.0.2",
        "prettier": "3.5.0",
        "rimraf": "6.0.1",
        "source-map-support": "0.5.21",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.7.3"
    },
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "prettier": "@doist/prettier-config"
}

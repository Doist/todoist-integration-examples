{
  "name": "todoist-lorem-ipsum",
  "version": "1.0.0",
  "description": "UI Extension for Todoist that generates Lorem Ipsum content",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "clean": "rimraf dist",
    "test": "jest --passWithNoTests",
    "format-check": "npx prettier --check .",
    "format-fix": "npx prettier --write .",
    "audit": "npx better-npm-audit audit --level=moderate",
    "integrity-check": "npm-run-all clean format-check lint-check ts-compile-check test",
    "ts-compile-check": "tsc --noEmit",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "database:run": "docker-compose -f docker-compose.db.yml up -d",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -d ormconfig.ts",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Doist/todoist-integrations-examples.git"
  },
  "author": "Doist Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Doist/todoist-integrations-examples/issues"
  },
  "homepage": "https://github.com/Doist/todoist-integrations-examples#readme",
  "dependencies": {
    "@doist/todoist-api-typescript": "2.0.1",
    "@doist/ui-extensions-core": "3.2.1",
    "@doist/ui-extensions-server": "1.1.0",
    "@nestjs/common": "9.0.11",
    "lorem-ipsum": "^2.0.8"
  },
  "devDependencies": {
    "@doist/tsconfig": "1.0.0",
    "@nestjs/cli": "9.1.2",
    "@nestjs/schematics": "9.0.3",
    "@types/express": "4.17.13",
    "@types/node": "18.6.2",
    "source-map-support": "0.5.21",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=16.11.1",
    "npm": ">=8.0.0"
  }
}
